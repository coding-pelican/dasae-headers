CompileFlags:
  Compiler: clang
  Add:
    - -xc
    - -std=c17
    - -Wall
    - -Wextra
    - -Werror
    - -funsigned-char
    - -fblocks
    - -fms-extensions
    - -Ic:/dasae/dev/dasae-headers/dh-examples/dh-ext/os/windows/wnd/src
    - -Ic:/dasae/dev/dasae-headers/dh-examples/dh-ext/os/windows/wnd/include
    - -I/mnt/c/dasae/dev/dasae-headers/dh-examples/dh-ext/os/windows/wnd/src
    - -I/mnt/c/dasae/dev/dasae-headers/dh-examples/dh-ext/os/windows/wnd/include
InlayHints:
  Enabled: true
  TypeNameLimit: 24
  DeducedTypes: true
  ParameterNames: true
  Designators: true
  BlockEnd: false
Hover:
  ShowAKA: true
---
If:
  PathMatch:
    - .*\.h
    - .*\.hh
    - .*\.hpp
    - .*\.hxx
CompileFlags:
  Add:
    - -Wno-unused-function
    - -Wno-unused-variable
    - -Wno-unused-const-variable
