CompileFlags:
  Compiler: clang
  Add:
    - -xc
    - -std=c17
    - -Wall
    - -Wextra
    - -Werror
    - -funsigned-char
    - -fblocks
    - -IC:\dev\dasae-headers\dh\include
    - -IC:\dev\dasae-headers\dh\libs\BlocksRuntime\include
    - -IC:\dev\dasae-headers\dh-examples\planet\src
    - -IC:\dev\dasae-headers\dh-examples\planet\include
    - -IC:\dev\dasae-headers\dh-examples\planet\lib\deps
InlayHints:
  Enabled: true
  TypeNameLimit: 24
  DeducedTypes: true
  ParameterNames: true
  Designators: true
  BlockEnd: false
Hover:
  ShowAKA: true
---
If:
  PathMatch:
    - .*\.h
    - .*\.hh
    - .*\.hpp
    - .*\.hxx
CompileFlags:
  Add:
    - -Wno-unused-function
    - -Wno-unused-variable
    - -Wno-unused-const-variable
