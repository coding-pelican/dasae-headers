CompileFlags:
  Compiler: clang
  Add:
    - -xc
    - -std=c17
    - -Wall
    - -Wextra
    - -Werror
    - -funsigned-char
    - -fblocks
    - -Ic:/dasae/dev/dasae-headers/dh/include
    - -Ic:/dasae/dev/dasae-headers/dh/libs/BlocksRuntime/include
    - -Ic:/dasae/dev/dasae-headers/dh-examples/engine/src
    - -Ic:/dasae/dev/dasae-headers/dh-examples/engine/include
    - -Ic:/dasae/dev/dasae-headers/dh-examples/engine/lib/deps
InlayHints:
  Enabled: true
  TypeNameLimit: 24
  DeducedTypes: true
  ParameterNames: true
  Designators: true
  BlockEnd: false
Hover:
  ShowAKA: true
---
If:
  PathMatch:
    - .*\.h
    - .*\.hh
    - .*\.hpp
    - .*\.hxx
CompileFlags:
  Add:
    - -Wno-unused-function
    - -Wno-unused-variable
    - -Wno-unused-const-variable
